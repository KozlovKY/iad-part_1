            window.sessionStorage.setItem('collapsed', false);
            $("span.fa", ".btn-history-task-text").removeClass("fa-caret-down").addClass("fa-caret-up");
        });

        if(window.sessionStorage.getItem('collapsed') === "true") {
            $('#task-text').collapse('hide');
